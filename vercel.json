{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "dramyheart.com"
        }
      ],
      "destination": "/site/dr-amy/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.dramyheart.com"
        }
      ],
      "destination": "/site/dr-amy/:path*"
    }
  ]
}
